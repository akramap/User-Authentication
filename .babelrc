{
  "presets": [
    ["env", {
      "targets": {
        "node": "8.9.0"
      }
    }],"es2015"
  ],
  "plugins": ["transform-object-rest-spread", [
    "module-resolver", {
      "cwd": "babelrc",
      "root": ["./"],
      "alias": {
        "localization": "./localization",
        "utils": "./utils",
        "helpers": "./src/helpers",
        "database": "./src/database",
        "@config": "./src/config"
      }
    }
  ]]
}
